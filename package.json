{
  "name": "scale-reader",
  "version": "0.3.0",
  "description": "Application that reads value off a scale from a local file stream",
  "main": "./lib/index.js",
  "bin": {
    "scale-reader": "./bin/cli.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf lib && babel src --out-dir lib",
    "test": "NODE_ENV=test istanbul cover _mocha",
    "lint": "eslint .",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Springworks/node-scale-reader.git"
  },
  "keywords": [
    "usb",
    "scale",
    "iot"
  ],
  "author": "Kristofer Sommestad <kristofer.sommestad@springworks.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Springworks/node-scale-reader/issues"
  },
  "homepage": "https://github.com/Springworks/node-scale-reader",
  "devDependencies": {
    "@springworks/test-harness": "1.2.2",
    "babel-cli": "6.3.17",
    "babel-core": "6.3.26",
    "babel-eslint": "4.1.3",
    "babel-plugin-transform-runtime": "6.3.13",
    "babel-plugin-transform-strict-mode": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "eslint": "3.11.0",
    "eslint-config-springworks": "3.2.0",
    "eslint-plugin-mocha": "1.0.0",
    "eslint-plugin-should-promised": "1.0.5",
    "istanbul": "0.4.1",
    "mocha": "2.3.4"
  },
  "dependencies": {
    "always-tail": "0.2.0",
    "babel-polyfill": "6.3.14",
    "commander": "2.9.0",
    "lodash.takeright": "3.0.0",
    "request": "2.65.0",
    "babel-runtime": "6.3.19"
  }
}
